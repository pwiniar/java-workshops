create table USERS (ID number(100) GENERATED AS IDENTITY NOT NULL, FIRST_NAME varchar(100), LAST_NAME varchar(100), PROFESSION varchar(100), TIMESTAMP timestamp default sysdate, PRIMARY KEY (ID));


